{
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "$": false,
    "angular": false
  },
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "extends": ["airbnb-base"],
  "rules": {
    "linebreak-style": "off",
    "no-bitwise": "off",
    "no-console": "off",
    "no-underscore-dangle": "off",
    "no-use-before-define": "off"
  }
}
